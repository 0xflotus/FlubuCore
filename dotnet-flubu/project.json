{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "warningsAsErrors": true,
    "outputName": "dotnet-flubu"
  },

  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0"
    },
    "Microsoft.Extensions.CommandLineUtils": "1.0.0",
    "Microsoft.CodeAnalysis.CSharp": "1.3.2",
    "Microsoft.CodeAnalysis.Common": "1.3.2",
    "Microsoft.CodeAnalysis.CSharp.Scripting": "1.3.2",
    "flubu.core": "1.0.0-*"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dotnet5.6"
    }
  },
  "packOptions": {
    "summary": "This extension for .NET Core CLI extends building capabilities.",
    "repository": {
      "type": "git",
      "url": "https://bitbucket.org/zoroz/flubu.core"
    },
    "tags": [
      ".NET Core CLI",
      "flubu",
      "project-json",
      "dotnet",
      "dotnet-flubu"
    ]
  }
}
