{
  "version": "1.1.1",
  "buildOptions": {
    "emitEntryPoint": true,
    "warningsAsErrors": true,
    "outputName": "dotnet-flubu",
    "additionalArguments": ["/ruleset:../Stylecop.ruleset"]
  },

  "dependencies": {
    "Microsoft.DotNet.Cli.Utils": "1.0.0-preview3-004056",
    "NLog.Extensions.Logging": "1.0.0-rtm-alpha2",
    "FlubuCore": "1.1.1*",
    "StyleCop.Analyzers": {
      "type": "build",
      "version": "1.0.0"
    },
    "Documentation.Analyser": "1.0.6166.19031",
    "Microsoft.Extensions.CommandLineUtils": "1.1.0",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.1.0",
    "Microsoft.Extensions.Configuration.Json": "1.1.0",
    "Microsoft.Extensions.DependencyInjection": "1.1.0",
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.1.0",
    "Microsoft.Extensions.Logging": "1.1.0",
    "Microsoft.Extensions.Logging.Console": "1.1.0",
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.1.0",
    "Microsoft.CodeAnalysis.CSharp": "2.0.0-rc",
    "Microsoft.CodeAnalysis.CSharp.Scripting": "2.0.0-rc",
    "Microsoft.CodeAnalysis.Scripting": "2.0.0-rc",
    "Microsoft.CodeAnalysis.Scripting.Common": "2.0.0-rc",
    "Microsoft.CodeAnalysis.Common": "2.0.0-rc"
  },

  "frameworks": {
    "netcoreapp1.1": {
      "imports": [
        "dotnet5.6",
        "netstandard1.6.1"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
        }
      }
    },
    "net46": {
      "dependencies": {
        "Microsoft.CodeAnalysis.CSharp.Scripting": "2.0.0-rc"
      }
    }
  }
}