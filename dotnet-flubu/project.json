{
  "version": "1.0.77",
  "buildOptions": {
    "emitEntryPoint": true,
    "warningsAsErrors": true,
    "outputName": "dotnet-flubu",
    "allowUnsafe": true,
    "additionalArguments": [ "/ruleset:../Stylecop.ruleset" ]
  },

  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.1"
    },
    "Microsoft.Extensions.CommandLineUtils": "1.0.0",
    "Microsoft.DotNet.Cli.Utils": "1.0.0-preview2-003121",
    "Microsoft.CodeAnalysis.CSharp": "1.3.2",
    "Microsoft.CodeAnalysis.Common": "1.3.2",
    "Microsoft.CodeAnalysis.CSharp.Scripting": "1.3.2",
    "NLog.Extensions.Logging": "1.0.0-rtm-alpha2",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "Microsoft.Extensions.Logging": "1.0.0",
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.0.0",
    "Microsoft.Extensions.DependencyInjection": "1.0.0",
    "FlubuCore": "1.0.77",
    "Microsoft.Extensions.Logging.Console": "1.0.0",
    "StyleCop.Analyzers": {
      "type": "build",
      "version": "1.0.0"
    },
    "Documentation.Analyser": {
      "type": "build",
      "version": "1.0.6092.26771"
    }
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "dnxcore50"
      ]
    }
  }
}