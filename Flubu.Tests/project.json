{
  "version": "1.0.0",
  "testRunner": "xunit",
  "buildOptions": {
    "allowUnsafe": true,
    "warningsAsErrors": true,
    "additionalArguments": [ "/ruleset:../Stylecop.ruleset" ],
    "copyToOutput": [
      "TestData/*"
    ]
  },
  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "Microsoft.NETCore.App": {
      "version": "1.0.1",
      "type": "platform"
    },
    "xunit": "2.1.0",
    "dotnet-test-xunit": "1.0.0-rc2-build10025",
    "moq": "4.6.25-alpha",
    "dotnet-flubu": "1.0.74",
    "FlubuCore": "1.0.74",
    "StyleCop.Analyzers": {
      "type": "build",
      "version": "1.0.0"
    },
    "Documentation.Analyser": {
      "type": "build",
      "version": "1.0.6092.26771"
    }
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net45+win8"
      ]
    }
  }
}
