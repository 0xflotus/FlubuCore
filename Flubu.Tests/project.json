{
  "version": "1.1.1",
  "testRunner": "xunit",
    "buildOptions": {
        "allowUnsafe": true,
        "warningsAsErrors": true,
        "additionalArguments": [ "/ruleset:../Stylecop.ruleset" ],
        "copyToOutput": [
            "TestData/*"
        ],
        "compile": {
            "exclude": [ "tmp", "TestData" ]
        }
    },
  "dependencies": {
    "System.Runtime.Serialization.Primitives": "4.3.0",
    "Microsoft.NETCore.App": {
      "version": "1.1.0",
      "type": "platform"
    },
    "xunit": "2.1.0",
    "moq": "4.6.25-alpha",
    "dotnet-flubu": "1.1.1*",
    "FlubuCore": "1.1.1*",
    "StyleCop.Analyzers": {
      "type": "build",
      "version": "1.0.0"
    },
    "dotnet-test-xunit": "2.2.0-preview2-build1029",
    "Documentation.Analyser": "1.0.6166.19031"
  },

  "frameworks": {
    "netcoreapp1.1": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "netstandard1.6.1",
        "portable-net45+win8"
      ]
    }
  }
}
